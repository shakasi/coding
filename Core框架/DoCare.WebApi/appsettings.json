{
  //"server.urls": "http://*:4889",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "DoCare": "Debug" //微软示例 没这行
    }
  },
  //"AllowedHosts": "*"//微软示例 有这行，没下面VNext
  "VNext": {
    "DbContexts": {
      "Default": {
        "DbContextTypeName": "VNext.Entity.DefaultDbContext,VNext.EntityFrameworkCore",
        "ConnectionString": "data source=Sqlites\\docare.db;",
        "DatabaseType": "Sqlite",
        "LazyLoadingProxiesEnabled": true,
        "AutoMigrationEnabled": true
      }
    },
    "Swagger": {
      "Endpoints": [
        {
          "Title": "发热门诊API",
          "Version": "docare",
          "Url": "/swagger/docare/swagger.json"
        }
      ],
      "RoutePrefix": "swagger",
      "MiniProfiler": true,
      "Enabled": true
    }
  }
}
